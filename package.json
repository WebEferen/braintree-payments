{
  "name": "braintree-payments",
  "version": "1.3.0",
  "description": "Typescript Braintree Payments",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "tslint": "./node_modules/.bin/tslint -p tsconfig.json || exit 0",
    "tslint-fix": "./node_modules/.bin/tslint -p tsconfig.json --fix || exit 0",
    "mocha": "./node_modules/.bin/mocha --timeout 5000 --reporter spec || exit 0",
    "build": "./node_modules/.bin/tsc",
    "test": "npm run clear && npm run test-build",
    "test-build": "npm run build && npm run mocha",
    "test-production": "npm run tslint && npm test",
    "test-coverage": "./node_modules/.bin/nyc --reporter=html --reporter=text npm run test-build || exit 0",
    "clear": "rm -r dist"
  },
  "nyc": {

  },
  "engines": {
    "node": "8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/WebEferen/braintree-payments.git"
  },
  "author": "Mike Makowski",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/WebEferen/braintree-payments/issues"
  },
  "homepage": "https://gitlab.com/WebEferen/braintree-payments#README",
  "dependencies": {
    "braintree": "^2.12.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
